{
  "name": "kirby-copilot",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "packageManager": "pnpm@8.14.3",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "build:assets": "vite build --config vite.config.assets.js && cp node_modules/pdfjs-dist/build/pdf.min.mjs assets/pdfjs.mjs && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs assets/pdfjs.worker.mjs",
    "build:playground": "PLAYGROUND=true pnpm run build && pnpm run build:assets",
    "readme:dev": "vitepress dev readme",
    "readme:build": "vitepress build readme",
    "readme:preview": "vitepress preview readme",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "archive": "pnpm run build && pnpm run build:assets && scripts/archive.sh",
    "release": "bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "autoprefixer": "^10.4.17",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "kirbyup": "^3.1.0",
    "modelfusion": "^0.133.0",
    "ohash": "^1.1.3",
    "pdfjs-dist": "^4.0.379",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "ufo": "^1.3.2",
    "vite": "^5.0.12",
    "vitepress": "1.0.0-rc.39"
  }
}
