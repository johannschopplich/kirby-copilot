{
  "name": "kirby-copilot",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.1",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "build:playground": "PLAYGROUND=true kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "archive": "git archive --format=zip --output=./kirby-copilot-$(git describe --tags).zip HEAD",
    "changelog": "changelogen --output",
    "release": "pnpm run build && bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "autoprefixer": "^10.4.16",
    "bumpp": "^9.2.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "kirbyup": "^3.1.0",
    "modelfusion": "^0.122.0",
    "ohash": "^1.1.3",
    "pdfjs-serverless": "^0.4.0",
    "prettier": "^3.2.1",
    "tailwindcss": "^3.4.1",
    "ufo": "^1.3.2"
  }
}
